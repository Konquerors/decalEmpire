extends ./layout/page.jade

block content
  
  .card
    .row
      .col-md-8
        #slider.flexslider
          ul.slides
            each val in [1, 2, 3, 4, 5, 6, 7, 8]
              - var model = 9;
              li
                img(src='assets/images/models/list/'+model+'/color/paint_1/'+val+'.png')
          //-ul
        //-#slider
        #carousel.flexslider
          ul.slides
            each val in [1, 2, 3, 4, 5, 6, 7, 8]
              - var model = 9;
              li
                img(src='assets/images/models/list/'+model+'/color/paint_1/'+val+'.png')
          //-ul
        //-#carousel
      //-.col-md-8
      .col-md-4
        h1 Amaze
        
        br
        
        include includes/product_details.jade
        
        hr
        
        strong Colors
        .paint
          .paint--colors.amaze
            .colors._1(data-color="1")
            .colors._2(data-color="2")
            .colors._3(data-color="3")
            .colors._4(data-color="4")
            .colors._5(data-color="5")
          //-.paint--colors
        //-.paint
        
        hr
        
        strong Variants
        ul
          li 1.3 E MT
          li 1.3 S AT
          li 1.3 V Navi MT
          li 1.3 V Navi AT
          li 1.3 S AT Modulo Aero
          li 1.3 S AT Modulo Utility
        
      //-.col-md-4
    //-.row
  //-.card

