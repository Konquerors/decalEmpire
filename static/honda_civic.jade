extends ./layout/page.jade

block content
  
  .card
    .row
      .col-md-8
        #slider.flexslider
          ul.slides
            each val in [1]
              - var model = 6;
              li
                img(src='assets/images/models/list/'+model+'/color/paint_1/'+val+'.png')
          //-ul
        //-#slider
        #carousel.flexslider
          ul.slides
            each val in [1]
              - var model = 6;
              li
                img(src='assets/images/models/list/'+model+'/color/paint_1/'+val+'.png')
          //-ul
        //-#carousel
      //-.col-md-8
      .col-md-4
        h1 Civic

        br
        
        include includes/product_details.jade
        
        hr
        
        strong Colors
        .paint
          .paint--colors.civic
            .colors._1(data-color="1")
            .colors._2(data-color="2")
            .colors._3(data-color="3")
            .colors._4(data-color="4")
            .colors._5(data-color="5")
            .colors._6(data-color="6")
            .colors._7(data-color="7")
            .colors._8(data-color="8")
            .colors._9(data-color="9")
          //-.paint--colors
        //-.paint
        
        hr
        
        strong Variants
        ul
          li 1.8 E
          li RS Turbo
        
